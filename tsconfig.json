{
    "compilerOptions": {
        "outDir": "./dist/",
        "target": "ES6",
        "module": "ES6",
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
    },
    "include": ["./src/**/*"],
    "exclude": ["node_modules"],
}